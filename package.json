{
  "name": "bravado-server",
  "version": "0.16.3",
  "description": "Server for the Bravado REST API framework",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "lint:changed": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=8.3"
  },
  "main": "lib/index.js",
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Losant/bravado-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/bravado-server.git"
  },
  "keywords": [
    "rest",
    "restful",
    "server",
    "api",
    "bravado"
  ],
  "lint-staged": {
    "*.js": "esw"
  },
  "dependencies": {
    "bravado-core": "^0.13.3",
    "bunyan": "^1.8.12",
    "clone": "^2.1.2",
    "commander": "^2.19.0",
    "convict": "^4.4.0",
    "debug": "^4.1.0",
    "defaults": "^1.0.3",
    "defined": "^1.0.0",
    "dotenv": "^6.1.0",
    "inflection": "^1.12.0",
    "object-assign": "^4.1.1",
    "q": "^1.5.1",
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "^1.3.4",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node",
    "rules": {
      "guard-for-in": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
